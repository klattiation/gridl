{
  "name": "gridl",
  "version": "0.0.1",
  "description": "Lightweight and easy to use library to handle data on 2D grids.",
  "main": "dist/gridlejs.min.js",
  "directories": {
    "lib": "./dist",
    "test": "./test"
  },
  "scripts": {
    "build": "WEBPACK_ENV=build webpack",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch",
    "test": "mocha-webpack --webpack-config webpack.test.config.js \"test/**/*.test.js\" --recursive",
    "test-and-watch": "mocha-webpack --webpack-config webpack.test.config.js \"test/**/*.test.js\" --recursive --watch",
    "coverage": "nyc mocha-webpack --timeout 30000 --webpack-config webpack.coverage.config.js \"test/**/*.test.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/klattiator/gridle.git"
  },
  "keywords": [
    "grid",
    "2d",
    "tile",
    "data",
    "structure",
    "rows",
    "columns"
  ],
  "author": "Sascha Klatt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/klattiation/gridl/issues"
  },
  "homepage": "https://github.com/klattiation/gridl#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.9.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "mocha": "^4.1.0",
    "mocha-webpack": "^1.0.1",
    "nyc": "^11.4.1",
    "path": "^0.12.7",
    "webpack": "^3.10.0",
    "webpack-istanbul-plugin": "0.0.5",
    "webpack-node-externals": "^1.6.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "exclude": [
      ".tmp",
      "test"
    ]
  }
}
